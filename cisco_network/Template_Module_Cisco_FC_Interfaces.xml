                            <delay>2m</delay>
                            <value_type>FLOAT</value_type>
                            <units>%</units>
                            <params>last(fcIfTxWaitCount.[{#SNMPINDEX}])/last(fcIfC3OutFrames.[{#SNMPINDEX}])*100</params>
                            <applications>
                                <application>
                                    <name>Fibre Channel</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Int. {#SNMPVALUEPHY} Description</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.31.1.1.1.18.{#SNMPINDEX}</snmp_oid>
                            <key>ifAlias.[{#SNMPINDEX}]</key>
                            <delay>1h</delay>
                            <history>30d</history>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <applications>
                                <application>
                                    <name>Fibre Channel</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Int. {#SNMPVALUEPHY} Input Speed</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.31.1.1.1.6.{#SNMPINDEX}</snmp_oid>
                            <key>ifHCInOctets.[{#SNMPINDEX}]</key>
                            <delay>2m</delay>
                            <history>30d</history>
                            <trends>120d</trends>
                            <units>bps</units>
                            <applications>
                                <application>
                                    <name>Fibre Channel</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Int. {#SNMPVALUEPHY} Output Speed</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.31.1.1.1.10.{#SNMPINDEX}</snmp_oid>
                            <key>ifHCOutOctets.[{#SNMPINDEX}]</key>
                            <delay>2m</delay>
                            <history>30d</history>
                            <trends>120d</trends>
                            <units>bps</units>
                            <applications>
                                <application>
                                    <name>Fibre Channel</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Int. {#SNMPVALUEPHY} ({#SNMPVALUEDESCR}) Rate and BB credits</name>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
