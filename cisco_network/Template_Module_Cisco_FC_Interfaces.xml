    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template_Module_Cisco_FC_Interfaces</template>
            <name>Template_Module_Cisco_FC_Interfaces</name>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Fibre Channel</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>FC Interfaces</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUEPHY},.1.3.6.1.2.1.2.2.1.2,{#SNMPVALUEDESCR},1.3.6.1.2.1.31.1.1.1.18,{#SNMPVALUEOPERSTATUS},1.3.6.1.2.1.2.2.1.8]</snmp_oid>
                    <key>discovery.SNMP.ifDescr</key>
                    <delay>1h</delay>
                    <filter>
                        <evaltype>AND</evaltype>
                        <conditions>
                            <condition>
                                <macro>{#SNMPVALUEDESCR}</macro>
                                <value>{$IF.DESCR.MATCH}</value>
                                <formulaid>A</formulaid>
                            </condition>
                            <condition>
                                <macro>{#SNMPVALUEDESCR}</macro>
                                <value>{$IF.DESCR.NOTMATCH}</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>B</formulaid>
                            </condition>
                            <condition>
                                <macro>{#SNMPVALUEOPERSTATUS}</macro>
                                <value>{$IF.OPERSTATUS.MATCH}</value>
                                <formulaid>C</formulaid>
                            </condition>
                            <condition>
                                <macro>{#SNMPVALUEOPERSTATUS}</macro>
                                <value>{$IF.OPERSTATUS.NOTMATCH}</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>D</formulaid>
                            </condition>
                            <condition>
                                <macro>{#SNMPVALUEPHY}</macro>
                                <value>{$IF.PHY.MATCH}</value>
                                <formulaid>E</formulaid>
                            </condition>
                            <condition>
                                <macro>{#SNMPVALUEPHY}</macro>
                                <value>{$IF.PHY.NOTMATCH}</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>F</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>Int. {#SNMPVALUEPHY} RX BB Credits →0 per second</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.9.9.289.1.2.1.1.41.{#SNMPINDEX}</snmp_oid>
                            <key>fcHCIfBBCreditTransistionToZero.[{#SNMPINDEX}]</key>
                            <delay>2m</delay>
                            <history>30d</history>
                            <trends>120d</trends>
                            <value_type>FLOAT</value_type>
                            <applications>
                                <application>
                                    <name>Fibre Channel</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Int. {#SNMPVALUEPHY} Class 3 Input Frames</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.9.9.289.1.2.3.1.1.{#SNMPINDEX}</snmp_oid>
                            <key>fcIfC3InFrames.[{#SNMPINDEX}]</key>
                            <delay>2m</delay>
                            <history>30d</history>
                            <trends>120d</trends>
                            <units>fps</units>
                            <applications>
                                <application>
                                    <name>Fibre Channel</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Int. {#SNMPVALUEPHY} Class 3 Output Frames</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.9.9.289.1.2.3.1.2.{#SNMPINDEX}</snmp_oid>
                            <key>fcIfC3OutFrames.[{#SNMPINDEX}]</key>
                            <delay>2m</delay>
                            <history>30d</history>
                            <trends>120d</trends>
                            <units>fps</units>
                            <applications>
                                <application>
                                    <name>Fibre Channel</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Int. {#SNMPVALUEPHY} RX BB Credits</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.9.9.289.1.2.5.1.1.{#SNMPINDEX}</snmp_oid>
                            <key>fcIfCurrRxBbCredit.[{#SNMPINDEX}]</key>
                            <delay>2m</delay>
                            <history>30d</history>
                            <trends>120d</trends>
                            <description>Количество BB Credits на приём (нулевое значение не является сбоем!)</description>
                            <applications>
                                <application>
                                    <name>Fibre Channel</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Int. {#SNMPVALUEPHY} TX BB Credits</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.9.9.289.1.2.5.1.2.{#SNMPINDEX}</snmp_oid>
                            <key>fcIfCurrTxBbCredit.[{#SNMPINDEX}]</key>
                            <delay>2m</delay>
                            <history>30d</history>
                            <trends>120d</trends>
                            <description>Количество BB Credits на передачу (нулевое значение не является сбоем!)</description>
                            <applications>
                                <application>
                                    <name>Fibre Channel</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Int. {#SNMPVALUEPHY} RX BB Credits →0 per packet</name>
                            <type>CALCULATED</type>
                            <key>fcIfTPPRate.[{#SNMPINDEX}]</key>
                            <delay>2m</delay>
                            <value_type>FLOAT</value_type>
                            <units>%</units>
                            <params>last(fcHCIfBBCreditTransistionToZero.[{#SNMPINDEX}])/last(fcIfC3InFrames.[{#SNMPINDEX}])*100</params>
                            <applications>
                                <application>
                                    <name>Fibre Channel</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Int. {#SNMPVALUEPHY} TX Wait count per second</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.9.9.289.1.2.1.1.15.{#SNMPINDEX}</snmp_oid>
                            <key>fcIfTxWaitCount.[{#SNMPINDEX}]</key>
                            <delay>2m</delay>
                            <history>30d</history>
                            <trends>120d</trends>
                            <value_type>FLOAT</value_type>
                            <applications>
                                <application>
                                    <name>Fibre Channel</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
