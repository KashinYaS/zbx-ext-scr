                    </graph_prototypes>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$IF.DESCR.MATCH}</macro>
                    <value>.+</value>
                </macro>
                <macro>
                    <macro>{$IF.DESCR.NOTMATCH}</macro>
                    <value>CHANGE_IF_NEEDED</value>
                </macro>
                <macro>
                    <macro>{$IF.OPERSTATUS.MATCH}</macro>
                    <value>1</value>
                </macro>
                <macro>
                    <macro>{$IF.OPERSTATUS.NOTMATCH}</macro>
                    <value>CHANGE_IF_NEEDED</value>
                </macro>
                <macro>
                    <macro>{$IF.PHY.MATCH}</macro>
                    <value>fc[0-9]+\/[0-9]+.*</value>
                </macro>
                <macro>
                    <macro>{$IF.PHY.NOTMATCH}</macro>
                    <value>CHANGE_IF_NEEDED</value>
                </macro>
