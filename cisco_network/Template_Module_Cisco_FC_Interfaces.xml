                                <graph_item>
                                    <sortorder>3</sortorder>
                                    <color>2774A4</color>
                                    <yaxisside>RIGHT</yaxisside>
                                    <item>
                                        <host>Template_Module_Cisco_FC_Interfaces</host>
                                        <key>fcIfCurrRxBbCredit.[{#SNMPINDEX}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>4</sortorder>
                                    <color>A54F10</color>
